            </TextBlock>
            <Button Grid.Column="1" Height="50" Width="250" VerticalAlignment="Top" HorizontalAlignment="Right"  Command="{Binding Path=TransitionToWelcome}" >
                <StackPanel Orientation="Horizontal">

                    <fa:ImageAwesome Icon="ArrowLeft" Margin="0 0 10 0" VerticalAlignment="Center" HorizontalAlignment="Right" Foreground="#0066B2" Height="20"/>
                    <TextBlock Text="Return to Home Screen" Foreground="#0066B2" Style="{StaticResource SmallTextStyle}" Margin="0 0"></TextBlock>

                </StackPanel>

            </Button>

        </Grid>
        <StackPanel Grid.Row="2" Orientation="Horizontal" VerticalAlignment="Center">
            <TextBlock  Style="{StaticResource SubHeaderStyle}">Type or scan your Exit code

            </TextBlock>
            <TextBox Height="60" FontSize="40" SelectedText="A" Width="350" Margin="20 0" x:Name="SearchBox"
                     controls:TextBoxHelper.ClearTextButton="True" Style="{StaticResource Textbox}"
                     Text="{Binding Criteria, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="stretch" VerticalAlignment="Top">
                <TextBox.InputBindings>
                    <KeyBinding Command="{Binding Path=FindByCriteria}" Key="Enter" />
                </TextBox.InputBindings>
            </TextBox>

            <Border Margin="5 10" VerticalAlignment="Center">
                <Button Width="100" Height="50" FontSize="22" Style="{DynamicResource AccentedSquareButtonStyle}"  Command="{Binding Path=FindByCriteria}" >
                    <StackPanel Orientation="Vertical">
                        <TextBlock TextAlignment="Center">Find</TextBlock>
                    </StackPanel>
                </Button>
            </Border>
        </StackPanel>

